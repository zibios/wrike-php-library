{
    "name": "zibios/wrike-php-library",
    "description": "Wrike PHP Library, generic implementation.",
    "keywords": ["wrike", "wrike.com", "api", "sdk", "library", "client", "REST", "HTTP", "php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Zbigniew Ślązak"
        }
    ],
    "require": {
        "php": ">=5.5,<7.2",
        "psr/http-message": ">=1.0,<2.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.0,<6.0"
    },
    "autoload": {
        "psr-4": {"Zibios\\WrikePhpLibrary\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": { "Zibios\\WrikePhpLibrary\\Tests\\": "tests/" }
    }
}
